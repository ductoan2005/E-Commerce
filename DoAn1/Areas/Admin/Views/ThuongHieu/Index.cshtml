@model IEnumerable<DoAn1.Models.ThuongHieu>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TenThuongHieu)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in ViewBag.ds) {
        <tr>
            <td>
                @item.TenThuongHieu
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ThuongHieuID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ThuongHieuID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ThuongHieuID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
